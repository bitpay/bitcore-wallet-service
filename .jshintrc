{
  "bitwise": true,
  "camelcase": true,
  "curly": true,
  "devel": false,
  "eqeqeq": true,
  "eqnull": false,
  "freeze": true,
  "funcscope": false,
  "immed": true,
  "indent": 2,
  "latedef": "nofunc",
  "maxcomplexity": 10,
  "maxdepth": 4,
  "maxerr": 99999, 
  "maxlen": 120,
  "maxparams": 4,
  "maxstatements": 15,
  "mocha": true,
  "newcap": true,
  "noarg": true,
  "node": true,
  "noempty": true,
  "nonew": true,
  "quotmark": "single",
  "regexp": true,
  "smarttabs": false,
  "strict": true,
  "trailing": true,
  "undef": true,
  "unused": true,
  "predef": [
    "after",
    "afterEach",
    "before",
    "beforeEach",
    "describe",
    "exports",
    "it",
    "xit",
    "module",
    "require"
  ]
}
